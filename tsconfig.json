{  "compilerOptions": {    "target": "ES2022",    "module": "ESNext",    "moduleResolution": "Bundler",    "lib": ["ES2022", "DOM", "DOM.Iterable"],    "jsx": "react-jsx",    "strict": true,    "noImplicitOverride": true,    "noImplicitReturns": true,    "noFallthroughCasesInSwitch": true,    "noUncheckedIndexedAccess": true,    "skipLibCheck": true  },  "include": ["src"]}
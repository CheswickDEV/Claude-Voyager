{"manifest_version": 3,"name": "Claude Voyager","version": "0.1.0","description": "Produktivitäts-Tools für claude.ai","icons": {"16": "public/icon-16.png","32": "public/icon-32.png","48": "public/icon-48.png","128": "public/icon-128.png"},"permissions": ["storage", "activeTab", "tabs"],"host_permissions": ["https://claude.ai/"],"background": {"service_worker": "src/pages/background/index.ts"},"content_scripts": [{"matches": ["https://claude.ai/"],"js": ["src/pages/content/index.ts"],"run_at": "document_idle"}],"action": {"default_popup": "src/pages/popup/index.html","default_title": "Claude Voyager"},"browser_specific_settings": {"gecko": {"id": "claude-voyager@example.com","strict_min_version": "121.0"}}}